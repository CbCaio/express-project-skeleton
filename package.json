{
  "name": "express-project-skeleton",
  "version": "0.0.1",
  "description": "Express-based project skeleton ",
  "main": "src/server.js",
  "scripts": {
    "start": "NODE_ENV=production pm2 start src/server.js -i max --name=skeleton-express-project",
    "local": "nodemon -- src/server.js",
    "pm2-monit": "pm2 monit",
    "pm2-kill": "pm2 kill",
    "test": "jest"
